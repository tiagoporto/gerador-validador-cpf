{
  "name": "gerador-validador-cpf-project",
  "private": true,
  "workspaces": [
    "packages/gerador-validador-cpf"
  ],
  "scripts": {
    "prepare": "husky",
    "start": "webpack serve --hot --config webpack.config.dev.js",
    "build": "rm -rf website && webpack --config webpack.config.prod.js",
    "checks": "npm run check-docs && npm run check-formatting && npm run typecheck && npm run lint:styles && npm run lint",
    "check-docs": "remark --frail .",
    "check-formatting": "prettier --check --ignore-unknown \"**/*\"",
    "check-formatting:fix": "npm run check-formatting -- --write",
    "typecheck": "tsc --project tsconfig.json",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint": "eslint --ext js,jsx,ts,tsx,md,mdx . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:bundle": "jest --config ./jest/es.config.js && jest --config ./jest/umd.config.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
