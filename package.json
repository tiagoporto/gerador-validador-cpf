{
  "name": "gerador-validador-cpf-root",
  "homepage": "https://tiagoporto.github.io/gerador-validador-cpf/",
  "private": true,
  "type": "module",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=24",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "version": ">=10",
      "onFail": "error"
    }
  },
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "postinstall": "$CI = true || (dependabot-config && editor-config)",
    "preinstall": "npx only-allow pnpm",
    "start": "webpack serve",
    "build": "rm -rf dist && webpack --config webpack.config.prod.mjs",
    "checks": "pnpm check-docs && pnpm format:check && pnpm typecheck && pnpm lint:styles && pnpm lint",
    "check-docs": "remark . --frail --ext md,markdown,mdx",
    "check-update": "pnpm update --interactive --latest && pnpm recursive run check-update",
    "format:check": "prettier --check --ignore-unknown \"**/*\"",
    "format": "npm run format:check -- --write",
    "typecheck": "tsc --project tsconfig.json",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "lint:styles": "stylelint \"**/*.{css,scss}\"",
    "lint:styles:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "coverage": "pnpm test --coverage",
    "test": "jest",
    "test:watch": "pnpm test --watch",
    "test:package": "jest --config jest.config.package.mjs",
    "test:mutation": "stryker run",
    "test:e2e": "CI=false ELECTRON_EXTRA_LAUNCH_ARGS=--remote-debugging-port=9222 start-server-and-test start http-get://localhost:8080 'cypress run --e2e'",
    "test:e2e:dev": "CI=false start-server-and-test \"pnpm start\" http-get://localhost:8080 'cypress open --e2e'",
    "pack-extract": "pnpm recursive run pack-extract"
  },
  "dependencies": {
    "@ctrl/react-adsense": "^2.1.0",
    "@types/ga-gtag": "^1.2.0",
    "gerador-validador-cnpj": "workspace:*",
    "gerador-validador-cpf": "workspace:*",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "normalize.css": "^8.0.1",
    "react": "^19.2.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.2.1",
    "react-github-corner": "^2.5.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.4.0",
    "react-imask": "^7.6.1",
    "react-syntax-highlighter": "^16.1.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@anolilab/multi-semantic-release": "^3.2.2",
    "@anolilab/semantic-release-pnpm": "^3.2.0",
    "@chromatic-com/cypress": "^0.11.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@cypress/webpack-preprocessor": "^7.0.2",
    "@sebbo2002/semantic-release-jsr": "3.1.0",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@stryker-mutator/typescript-checker": "^9.4.0",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@tiagoporto/dependabot-config": "^2.5.0",
    "@tiagoporto/editor-config": "^2.1.0",
    "@tiagoporto/eslint-config": "^6.4.0",
    "@tiagoporto/prettier-config": "^2.1.0",
    "@tiagoporto/remark-config": "^1.11.0",
    "@tiagoporto/stylelint-config": "^1.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/webpack-env": "^1.18.8",
    "chromatic": "^13.3.4",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.3",
    "cypress": "^15.7.1",
    "eslint": "^9.39.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-jest": "^29.2.1",
    "html-webpack-plugin": "^5.6.5",
    "jest": "^30.2.0",
    "lefthook": "^2.0.9",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.5.0",
    "prettier": "^3.7.4",
    "remark": "^15.0.1",
    "remark-cli": "^12.0.1",
    "sass-embedded": "^1.93.3",
    "sass-loader": "^16.0.6",
    "semantic-release": "^25.0.2",
    "start-server-and-test": "^2.1.3",
    "style-loader": "^4.0.0",
    "stylelint": "^16.26.1",
    "swc-loader": "^0.2.6",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^7.4.0"
  },
  "pnpm": {
    "overrides": {
      "react-copy-to-clipboard>react": ">=19",
      "react-helmet-async>react": ">=19",
      "conventional-changelog-conventionalcommits": ">=8.0.0"
    },
    "onlyBuiltDependencies": [
      "cypress",
      "lefthook"
    ]
  }
}
