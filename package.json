{
  "name": "gerador-validador-cpf",
  "version": "5.0.1",
  "description": "Biblioteca JS open-source para gerar, validar e formatar CPF.",
  "homepage": "http://tiagoporto.github.io/gerador-validador-cpf",
  "keywords": [
    "cpf",
    "gerador",
    "generator",
    "generates",
    "validador",
    "valida",
    "validates"
  ],
  "author": {
    "name": "Tiago Porto",
    "url": "http://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoporto/gerador-validador-cpf.git"
  },
  "main": "./dist/CPF.umd.js",
  "module": "./dist/CPF.es.js",
  "typings": "./dist/CPF.d.ts",
  "files": [
    "/dist",
    "/README-en.md"
  ],
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack.config.dev.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.md,.mdx ./src --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "checks": "npm run check-formatting && npm run check-typing && npm run lint",
    "check-typing": "tsc --noEmit",
    "check-formatting": "prettier './**/*' --check --ignore-unknown",
    "formatting:fix": "prettier './**/*' --write --ignore-unknown",
    "test": "jest",
    "test:bundle": "jest --config ./jest/es.config.js && jest --config ./jest/umd.config.js && jest --config ./jest/umd.min.config.js",
    "test:watch": "npm test -- --watch",
    "test:mutation": "stryker run",
    "test:mutation-report": "stryker run --reporters dashboard",
    "test:coverage": "npm test -- --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "types": "tsc ./src/lib/CPF.ts --declarationDir dist --emitDeclarationOnly --declaration",
    "build:site": "rm -rf website && webpack --config webpack.config.prod.js",
    "build": "rollup --config",
    "deploy": "npm run build:site && npx gh-pages -d website",
    "prepublishOnly": "rm -rf dist && npm run build && npm run types",
    "postpublish": "npm run tag && git push --tags",
    "tag": "git tag v$npm_package_version"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@stryker-mutator/core": "^3.2.4",
    "@stryker-mutator/jest-runner": "^3.2.4",
    "@stryker-mutator/typescript": "^3.2.4",
    "@types/highlight.js": "^10.1.0",
    "@types/jest": "^26.0.19",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "coveralls": "^3.1.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.0.0-rc.0",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "file-loader": "^6.2.0",
    "highlight.js": "^10.4.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "i18next": "^19.8.4",
    "jest": "^26.6.3",
    "json-map-keys-loader": "^1.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "normalize.css": "^8.0.1",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.1",
    "react-ga": "^2.7.0",
    "react-github-corner": "^2.5.0",
    "react-i18next": "^11.8.3",
    "react-imask": "^6.0.5",
    "react-toastify": "^6.2.0",
    "rollup": "^2.34.2",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.1",
    "typescript": "^4.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.3"
  }
}
