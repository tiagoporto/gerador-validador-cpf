{
  "name": "gerador-validador-cpf-project",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22",
    "npm": "uses pnpm",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "start": "webpack serve --config webpack.config.dev.mjs",
    "build": "rm -rf dist && webpack --config webpack.config.prod.mjs",
    "checks": "pnpm check-docs && pnpm check-formatting && pnpm typecheck && pnpm lint:styles && pnpm lint",
    "check-docs": "remark .",
    "formatting:check": "prettier --check --ignore-unknown \"**/*\"",
    "formatting": "pnpm check-formatting -- --write",
    "typecheck": "tsc --project tsconfig.json",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint": "eslint --ext js,jsx,mjs,ts,tsx,md,mdx . --max-warnings 0",
    "lint:fix": "pnpm lint -- --fix",
    "coverage": "npm test -- --coverage",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:package": "jest --config jest.config.package.mjs",
    "test:mutation": "stryker run"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/jest-runner": "^8.7.1",
    "@stryker-mutator/typescript-checker": "^8.7.1",
    "@swc/core": "^1.10.4",
    "@swc/jest": "^0.2.37",
    "@tiagoporto/prettier-config": "^1.0.0",
    "@tiagoporto/remark-config": "^1.1.0",
    "@tiagoporto/stylelint-config": "^1.0.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-testing-library": "^6.0.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "gerador-validador-cpf": "workspace:*",
    "highlight.js": "^11.6.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
