{
  "name": "gerador-validador-cpf-project",
  "private": true,
  "workspaces": [
    "packages/gerador-validador-cpf"
  ],
  "scripts": {
    "prepare": "husky",
    "start": "webpack serve --config webpack.config.dev.mjs",
    "build": "rm -rf dist && webpack --config webpack.config.prod.mjs",
    "checks": "npm run check-docs && npm run check-formatting && npm run typecheck && npm run lint:styles && npm run lint",
    "check-docs": "remark --frail .",
    "check-formatting": "prettier --check --ignore-unknown \"**/*\"",
    "check-formatting:fix": "npm run check-formatting -- --write",
    "typecheck": "tsc --project tsconfig.json",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint": "eslint --ext js,jsx,mjs,ts,tsx,md,mdx . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:bundle": "jest --config ./jest/es.config.js && jest --config ./jest/umd.config.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@swc/core": "^1.10.4",
    "@tiagoporto/prettier-config": "^1.0.0",
    "@tiagoporto/remark-config": "^1.0.0",
    "@types/jest": "^29.2.0",
    "@types/react": "^19.0.2",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-testing-library": "^6.0.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "highlight.js": "^11.6.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "i18next": "^24.2.0",
    "jest": "^29.2.1",
    "lint-staged": "^15.3.0",
    "mini-css-extract-plugin": "^2.9.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.0.0",
    "react-ga": "^3.3.1",
    "react-github-corner": "^2.5.0",
    "react-i18next": "^15.4.0",
    "react-imask": "^7.6.1",
    "react-toastify": "^11.0.2",
